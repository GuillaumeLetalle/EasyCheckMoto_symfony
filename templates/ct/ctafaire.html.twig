{% extends 'base.html.twig' %}

{% block body %}

    <table class="table">
        <thead>
            <tr>
                <th>Debut</th>
            </tr>
        </thead>
        <tbody>
        
        {% for ct in cts %}
        {% if ct.cteffectue == false %}
            <tr>
                <td>{{ ct.debut ? ct.debut|date('d-m-Y H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_ct_edit', {'id': ct.id}) }}">Prendre en charge</a>
                </td>
            </tr>
        {% endif %}
        {% endfor %}
        
        </tbody>
    </table>

{% endblock %}
